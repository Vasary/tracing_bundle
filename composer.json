{
    "name": "vasary/tracer-bundle",
    "description": "Tracer id bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4 || >= 8.0",
        "symfony/framework-bundle": "3.4|^4.0|^5.0|^5.1",
        "symfony/yaml": "3.4|^4.0|^5.0|^5.1",
        "vasary/x-trace-id": "^0.1.2"
    },
    "autoload": {
        "psr-4": {
            "Vasary\\TracingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vasary\\TracingBundle\\Tests\\": "tests/"
        }
    }
}
